#!/bin/sh

cli=/Applications/Karabiner.app/Contents/Library/bin/karabiner

$cli set repeat.initial_wait 250
/bin/echo -n .
$cli set remap.fn_consumer_to_fkeys_f7 1
/bin/echo -n .
$cli set remap.controlL2fn 1
/bin/echo -n .
$cli set remap.fn_consumer_to_fkeys_f10 1
/bin/echo -n .
$cli set remap.fn_consumer_to_fkeys_f1 1
/bin/echo -n .
$cli set repeat.wait 30
/bin/echo -n .
$cli set private.resizefull 1
/bin/echo -n .
$cli set private.resizeleft 1
/bin/echo -n .
$cli set remap.fn_consumer_to_fkeys_f3 1
/bin/echo -n .
$cli set private.resizeright 1
/bin/echo -n .
$cli set remap.fn2controlL 1
/bin/echo -n .
$cli set remap.fn_consumer_to_fkeys_f5 1
/bin/echo -n .
/bin/echo
